unlet! skip_defaults_vim
source $VIMRUNTIME/defaults.vim
colorscheme dracula
set breakindent
set cursorline
set expandtab
set hlsearch
set list
set mouse=
set number
let g:airline#extensions#tabline#enabled = 1
let g:airline#extensions#tabline#tab_nr_type = 1
let g:airline_powerline_fonts = 1
let g:auto_save = 1
